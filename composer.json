{
    "name": "ahmetsabri/gaza-validation-generator",
    "description": "A Laravel package to generate validation rules for database tables",
    "type": "library",
    "license": "MIT",
    "version": "1",
    "autoload": {
        "psr-4": {
            "Gaza\\ValidationGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ahmed Sabry"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/console": "^10|^11",
        "illuminate/database": "^10|^11",
        "illuminate/support": "^10|^11"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "tightenco/duster": "^2.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gaza\\ValidationGenerator\\ValidationGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}